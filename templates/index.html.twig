<script>
    (function(document, window, $){
        Dropzone.autoDiscover = false;
        'use strict';

        var Site = window.Site;
        $(document).ready(function(){
            Site.run();
        });
    })
    (document, window, jQuery);


    $( document ).ready(function() {
        //alert($("#dashUserwidget").height())
        initDropzone();
        $('#toggleUpload').click(function () {
            $('#site-sidebar').toggleClass( 'slidePanel-show', 1000 );
        });


        $(document).on("click", ".open-AddBookDialog", function () {

            var myBookId = $(this).data('id');
            $(".modal-body #userId").val( myBookId );
            $("#todoModal").show();
        });

        $("#dropzone-form").dropzone({ /* options */ });
    });

    function initDropzone(){
        var formElement = document.querySelector('.global');
        if (!formElement){
            return;
        }
        var dropzone = new Dropzone(formElement,{
            paramName: 'test',
            thumbnailWidth: 80,
            thumbnailHeight: 80,
            dictDefaultMessage: 'Dateien hier ablegen',
            height: 800,
            init: function (){
                this.on('error', function(file, data){
                    this.emit('error', file, data.detail);
                });
            }
        });
    }


</script>
